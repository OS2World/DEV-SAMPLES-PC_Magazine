


@ECHO OFF
SET FAXFILE$=%1
IF (%FAXFILE$%)==() GOTO GET_FILE
IF EXIST %FAXFILE$% GOTO HAVE_FILE
:GET_FILE
MENU /N FAX2.MNU FAX2.TPL FAX$.BAT
CALL FAX$.BAT
DEL FAX$.BAT
IF (%FAXFILE$%)==() GOTO END
IF NOT EXIST %FAXFILE$% GOTO GET_FILE
:HAVE_FILE
MENU /N FAX1.MNU FAX1.TPL FAX$.CVR
COPY FAX$.CVR+%FAXFILE%
FAXPRG /FFAX$.CVR /N /P /TTEL.FIL
DEL FAX$.CVR
SET FAXFILE$=
:END

